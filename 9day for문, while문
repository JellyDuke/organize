		for (int i = 2; i <= 9; i++) {
			if (i < 7) { //7보다 클때만 해라
				continue; // 위랑 동일
			}
			System.out.println();
			for (int j = 1; j <= 9; j++) {
				System.out.println(i + "X" + j + "=" + (i * j));
			}
		}
	}
